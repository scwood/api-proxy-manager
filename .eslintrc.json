{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true,
    "node": true,
    "node": true,
    "mocha": true,
    "jquery": true
  },
  "globals": {
    "angular":true
  },
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "script"
  },
  "rules": {
    "no-var":"off",
    "comma-dangle":"off",
    "require-jsdoc":"off",
    "max-len":["warn", 120],
    "quotes":["error", "double"]
  }
}
